{
  "Code block": {
    "prefix": ["code"],
    "body": ["{{{",
        "# ${1:language} #",
        "${2:code}",
        "}}}"
    ],
    "description": "Adds a code block"
  },
  "Math block": {
    "prefix": ["math"],
    "body": ["{{",
